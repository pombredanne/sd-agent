        <allowed-os-versions>
            <os-version min="10.10"/>
        </allowed-os-versions>
    </volume-check>
    <choices-outline>
        <line choice="default">
            <line choice="com.serverdensity.agent-service"/>
        </line>
    </choices-outline>
    <choice id="default"/>
    <choice id="com.serverdensity.agent-service" visible="false">
        <pkg-ref id="com.serverdensity.agent-service"/>
    </choice>
    <pkg-ref id="com.serverdensity.agent-service" version="2.1.5" onConclusion="none">Server%20Density%20Agent%20Service.pkg</pkg-ref>
